<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!--SolidColorBrushs-->
    <SolidColorBrush x:Key="SCBXPPrimary">#1b212d</SolidColorBrush>
    <SolidColorBrush x:Key="SCBXPSecondary">#2b3240</SolidColorBrush>
    <SolidColorBrush x:Key="SCBXPWhite">#d7dde6</SolidColorBrush>
    <SolidColorBrush x:Key="SCBXPLightGrey">#7c8494</SolidColorBrush>
    <SolidColorBrush x:Key="SCBXPYellow">#FFF80A</SolidColorBrush>
    <SolidColorBrush x:Key="SCBXPRed">#EB1D36</SolidColorBrush>
    <SolidColorBrush x:Key="SCBXPBlue">#3AB4F2</SolidColorBrush>
    <SolidColorBrush x:Key="SCBXPGreen">#3EC70B</SolidColorBrush>

    <!--Colors-->
    <Color x:Key="CXPPrimary">#1b212d</Color>
    <Color x:Key="CXPSecondary">#2b3240</Color>
    <Color x:Key="CXPWhite">#d7dde6</Color>
    <Color x:Key="CXPLightGrey">#7c8494</Color>
    <Color x:Key="CXPYellow">#FFF80A</Color>
    <Color x:Key="CXPRed">#EB1D36</Color>
    <Color x:Key="CXPBlue">#3AB4F2</Color>
    <Color x:Key="CXPGreen">#3EC70B</Color>
</ResourceDictionary>